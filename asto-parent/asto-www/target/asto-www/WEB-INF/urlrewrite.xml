<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>

	<rule>
		<from>^/goods(\d+).htm$</from>
		<to>/goods/goods.htm?id=$1</to>
	</rule>

	<rule>
		<from>^/compInfo(\d+).htm$</from>
		<to>/compInfo.htm?id=$1</to>
	</rule>

	<!-- 关键字 -->
	<rule>
		<from>^/list-([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/list.htm?title=$1</to>
	</rule>
	
	<!-- 关键字 -->
	<rule>
		<from>^/list-([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/list.htm?title=$1</to>
	</rule>
	
	<!-- 关键字 -->
	<rule>
		<from>^/list-([0-9,a-z,A-Z,%,+]*)-startIndex(\d+)-pageSize(\d+).htm$</from>
		<to>/list.htm?title=$1&amp;startIndex=$2&amp;pageSize=$3</to>
	</rule>
	
</urlrewrite>